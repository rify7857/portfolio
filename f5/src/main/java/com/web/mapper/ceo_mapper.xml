<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.ceo">
	<select id="selectStore" parameterType="java.lang.String" resultType="java.lang.String">
		select StoreIdx from ceo where memberId = #{memberId}
	</select>
</mapper>

